FROM DETALLE_ALMACEN_PRODUCTOS DT INNER JOIN PRODUCTO PR ON
DT.ID_PRODUCTO=PR.ID_PRODUCTO INNER JOIN TIENDA TI ON
DT.ID_TIENDA = TI.ID_TIENDA INNER JOIN UBICACION_FISICA UBI
